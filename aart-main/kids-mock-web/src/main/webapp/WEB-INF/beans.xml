<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:jaxws="http://cxf.apache.org/jaxws"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd"
    xmlns="http://www.springframework.org/schema/beans" >

    <jaxws:endpoint
        xmlns:tns="http://tempuri.org/kids_data/KIDS_WebService"
        id="kidswebservicesoap"
        address="/KIDS_WebServiceSoap12"
        endpointName="tns:KIDS_WebServiceSoap12"
        implementor="edu.ku.cete.ksde.kids.service.KIDSWebServiceSoapImpl"
        serviceName="tns:KIDS_WebService"
        wsdlLocation="WEB-INF/kids_webservice.wsdl">
        <jaxws:features>
            <bean class="org.apache.cxf.feature.LoggingFeature" />
        </jaxws:features>
    </jaxws:endpoint>

</beans>