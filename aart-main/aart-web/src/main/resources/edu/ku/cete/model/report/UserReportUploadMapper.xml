  <select id="selectLatestUploadedDataByUser" resultMap="BaseResultMap">
 	 SELECT 
    	uru.id as id, c.categoryname as fileTypeCategoryName, uru.uploadedfilename as uploadedFileName, 
    	au.firstname || ' ' || au.surname as userName, uru.createddate as createdDate
 	FROM userreportupload uru
    	JOIN aartuser au ON uru.aartuserid = au.id
    	JOIN category c ON uru.filetypeid = c.id
 	WHERE (uru.id,c.categoryname) in (
		SELECT 
			max(uru.id) as id, c.categoryname as fileTypeCategoryName
		FROM userreportupload uru		
			JOIN category c ON uru.filetypeid = c.id
		GROUP BY fileTypeCategoryName)
	
