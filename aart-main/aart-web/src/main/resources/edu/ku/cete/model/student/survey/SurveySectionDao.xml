  <select id="getSurveyRootSections" resultMap="BaseResultMap">
  	select id, surveysectionname, sectionorder, surveysectioncode, surveysectiondescription
  	from surveysection
  	where parentsurveysectionid is null and activeflag is true
  	order by sectionorder
  </select>
  
  <select id="getSurveySubSections" resultMap="BaseResultMap">
  	select distinct ss.parentsurveysectionid, ss.id , ss.sectionorder, ss.surveysectionname,ss.surveysectiondescription, sl.globalpagenum,
  		ss.instructionnote
		from surveysection ss
		join surveylabel sl on sl.surveysectionid=ss.id
		where ss.parentsurveysectionid is not null and ss.activeflag is true and sl.activeflag is true
		order by sl.globalpagenum
  </select>
  
  <select id="getActiveRootSections" resultType="INTEGER" parameterType="java.lang.Long">
  	select distinct sectionorder from surveysection 
  		where id in (
			select distinct ss.parentsurveysectionid from surveysection ss, surveylabel sl 
			where ss.id = sl.surveysectionid and sl.globalpagenum in (
			select globalpagenum from  surveypagestatus where iscompleted = true and activeflag = true and surveyid= #{surveyId}
		) and ss.activeflag = true and sl.activeflag = true) 
	order by sectionorder
  </select>
  
  <select id="getFirstContactActivePageCount" resultType="INTEGER">
  	select count(distinct globalpagenum) from surveylabel where activeflag is true
  </select>
