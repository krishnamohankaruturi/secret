  <select id="selectAllTestTypes" resultType="edu.ku.cete.domain.TestType">
		SELECT * from testtype where activeflag=true
  </select>
	
	<select id="selectAllSubjectAreas" resultType="edu.ku.cete.domain.enrollment.SubjectArea">
		SELECT * from subjectarea where activeflag=true
  </select>
	<select id="selectValidTestTypes" resultType="edu.ku.cete.domain.TestType">
		select distinct(tt.id) as id,testtypecode,testtypename  from testtype tt 
 		inner join testtypesubjectarea ttsa on tt.id=ttsa.testtypeid
 		inner join contentareatesttypesubjectarea cattsa on ttsa.id=cattsa.testtypesubjectareaid
 		inner join contentarea ca on ca.id=cattsa.contentareaid
 		where upper(ca.abbreviatedname)=upper(#{subject}) 
 			and tt.activeflag=true and ttsa.activeflag=true 
 			and cattsa.activeflag=true and ca.activeflag=true 
	</select>
  
	<select id="getTestTypeBySubjectAssesment" resultType="edu.ku.cete.domain.TestType">
		select distinct tt.id as id,testtypecode,testtypename  from testtype tt 
 		join testtypesubjectarea ttsa on tt.id=ttsa.testtypeid 
		join subjectarea sa on sa.id = ttsa.subjectareaid
		join contentareatesttypesubjectarea cattsa on ttsa.id=cattsa.testtypesubjectareaid 
		join contentarea ca on ca.id=cattsa.contentareaid 
		join assessment asmt on asmt.id = ttsa.assessmentid
		join testingprogram tp on tp.id=asmt.testingprogramid
		join assessmentprogram ap on ap.id = tp.assessmentprogramid
 		where upper(ca.abbreviatedname)=upper(#{SubjectCode})
 			and ap.id =  #{assessmentProgramId,jdbcType=BIGINT}
 			and tt.activeflag=true and ttsa.activeflag=true 
 			and sa.activeflag=true and cattsa.activeflag = true
 			and ca.activeflag =true and asmt.activeflag = true
 			and tp.activeflag = true and ap.activeflag = true
 		order by testtypename asc 
	</select>
			
	<select id="selectTestTypesBySubjectCode" resultType="edu.ku.cete.domain.TestType">
 		select distinct tt.id as id,testtypecode,testtypename  from testtype tt 
 		inner join testtypesubjectarea ttsa on tt.id=ttsa.testtypeid
 		inner join subjectarea sa on sa.id=ttsa.subjectareaid
 		where upper(sa.subjectareacode)=upper(#{subjectCode}) 
 			and tt.activeflag=true and ttsa.activeflag=true 
 			and sa.activeflag=true 
	</select>
	
