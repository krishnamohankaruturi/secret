  <select id="checkCompletionBySurveyAndPageNum" resultType="java.lang.Boolean" >
	Select false from surveylabel sl 
	where sl.globalpagenum = #{globalPageNum}
		<!-- the question has no response -->
		and not exists (select 1 from studentsurveyresponse ssr,surveyresponse sr
	 		where
	  			ssr.surveyresponseid=sr.id and
	  			sr.labelid = sl.id and 
	  			ssr.activeflag = true and
	  			sr.activeflag = true and
	  			ssr.surveyid =#{surveyId,jdbcType=BIGINT}
	 			)
			<!--the unanswered question has no prerequisite -->
		<if test="coreSetQuestions == true">
			and not exists (select 1 from surveylabelprerequisite slr
	 		where slr.surveylabelid = sl.id and slr.activeflag = true)
		</if>
		<if test="coreSetQuestions != true">
			and not exists (select 1 from surveylabel where 
			 labelnumber ~ (select '^(' || array_to_string(array_agg(case when position('_' in labelnumber)>0 
			 	then substring(labelnumber from 1 for position('_' in labelnumber)) else labelnumber end),'|') || ')'
			 from surveylabel where id in (select surveylabelid 
			 from surveylabelprerequisite where prerequisitecondition='or' and activeflag is true) and activeflag is true ) 
			 and globalpagenum = #{globalPageNum})
		</if>
	 	<if test="coreSetQuestions == true">
			and optional = false
		</if>
	 	and sl.activeflag = true
	 	limit 1
  </select> 
  
