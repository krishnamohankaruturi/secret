  <select id="selectByAssessmentId" resultMap="BaseResultMap" parameterType="java.lang.Long" >
    select distinct tt.id, tt.testtypecode, tt.testtypename, tt.assessmentid, tt.createduser, tt.createdate, tt.modifieddate, tt.modifieduser
    from testtype tt 
    inner join testtypesubjectarea ttsa on tt.id=ttsa.testtypeid
    where ttsa.activeflag is true and tt.activeflag is true 
    and ttsa.assessmentid = #{assessmentId,jdbcType=BIGINT} 
    order by tt.testtypename 
  </select>
  
  <select id="getByAssessmentProgram" resultMap="BaseResultMap" parameterType="java.lang.Long" >      
	    SELECT tt.id,testtypename,testtypecode from testtype as tt
        inner join assessment as ass ON ass.id = tt.assessmentid and ass.activeflag = true
        inner join testingprogram as tp ON tp.id = ass.testingprogramid and tp.activeflag = true
        WHERE tp. assessmentprogramid = #{assessmentProgramId,jdbcType=BIGINT} and tt.activeflag = true
  </select>
	
  <select id="getCPASSTestTypesForReportsByTestTypeCode" resultMap="BaseResultMap">
     select distinct ca.name,
     ca.abbreviatedname as subjectCode,
     gc.name,
     gc.abbreviatedname as gradeCode,
     tt.id,
     tt.testtypename,
     tt.testtypecode
    from contentareatesttypesubjectarea cttsa
    inner join testtypesubjectarea ttsa on cttsa.testtypesubjectareaid=ttsa.id
    inner join gradecontentareatesttypesubjectarea gcttsa on gcttsa.contentareatesttypesubjectareaid=cttsa.id
    inner join gradecourse gc on gcttsa.gradecourseid=gc.id
    inner join contentarea ca on cttsa.contentareaid = ca.id
    inner join testtype tt on tt.id = ttsa.testtypeid
   where 
   ttsa.assessmentid in (select id from assessment where testingprogramid = #{testingProgramId,jdbcType=BIGINT} 
   and assessmentcode = 'GL')  and 
   tt.testtypename NOT ILIKE '%CCQ'
   and testtypecode = #{testType}
   and gc.activeflag is true and gcttsa.activeflag is true and ttsa.activeflag is true and cttsa.activeflag is true
   order by ca.name,gc.name
          
  </select>  
