    <select id="getDLMActiveUserSecurityAgreementInfo" resultType="edu.ku.cete.domain.UserSecurityAgreement">	
		SELECT * FROM (SELECT * FROM usersecurityagreement usa 
		INNER JOIN assessmentprogram agrap on agrap.id = usa.assessmentprogramid 
		WHERE agrap.activeflag is true
		AND to_date(usa.expiredate,'MM/DD/YYYY') > date(current_timestamp)
		AND usa.aartuserid = #{aartUserId,jdbcType=BIGINT} 
		AND usa.assessmentprogramid = #{assessmentProgramId,jdbcType=BIGINT}
	 	AND usa.schoolyear = #{currentSchoolYear,jdbcType=BIGINT} 
	 	ORDER BY usa.agreementsigneddate DESC limit 1) as latestAgreement where latestAgreement.agreementelection is true
   </select>
   
    <select id="getSecurityAgreement" resultType="edu.ku.cete.domain.UserSecurityAgreement">
select distinct * from usersecurityagreement u 
inner join 
(select aartuserid,max(expiredate) as maxdate from usersecurityagreement where aartuserid=#{aartUserId} group by aartuserid) a
on a.aartuserid=u.aartuserid and a.maxdate=u.expiredate
 </select>
